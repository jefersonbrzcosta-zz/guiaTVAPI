{
  "name": "apiguide",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "build-ts": "npm test && tsc",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/database/strategy.ts",
    "postinstall": "npm run build-ts",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/tests/*.test.ts'"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "got": "^11.8.2",
    "jsdom": "^16.5.3",
    "mongoose": "^5.12.12"
  },
  "engines": {
    "node": "14.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^8.4.0",
    "prettier": "^2.3.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
}
